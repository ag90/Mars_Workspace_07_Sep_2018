package OOP;

public class Datatype {

	
	public static void main(String[] args) {


		char i=65;
		System.out.println(i);
		
		char w=90;
		System.out.println(w);
		
		char p=97;
		System.out.println(p);
		
		char n=122;
		System.out.println(n);
		
		char j='@';
		System.out.println(j);
		
		int k=90;
		System.out.println(k);
		
		long y=1111111111;
		System.out.println(y);
		
		boolean d=true;
		System.out.println(d);
		
		boolean q=true;
		System.out.println(q);
		
		double x=111.10228;
		System.out.println(x);
		
		byte h=122;
		System.out.println(h);//All are premitive data types becuase they are holding the values.
		
		String str1="AVI";
		System.out.println(str1);
		
		String str=new String("AVIGUPTA");
		System.out.println(str);
		
		Integer ten=new Integer(8);
		System.out.println(ten);//All are non primitve or reference variable because they are store the references.
		
		
	
				
		
	}

}
