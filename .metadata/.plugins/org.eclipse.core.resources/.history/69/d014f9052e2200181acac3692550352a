package Collection;

import java.util.ArrayList;

public class ArrayListConcept {

	public static void main(String[] args) {


		ArrayList ar1=new ArrayList();
		
		//to add the element in array list
		ar1.add("Test");//0
		ar1.add("DOM");//1
		ar1.add("Selenium");//2
		ar1.add('R');//3
		ar1.add(100);//4
		ar1.add(99.20);//5
		
		
		ArrayList ar2=new ArrayList();
		
		ar2.add("Data Configuration");
		ar2.add("Page Object Model");
		
		//to check the size of array list
		
		System.out.println(ar1.size());
		System.out.println(ar2.size());
		
		System.out.println("***********************");
		
         ArrayList ar3=new ArrayList();
		
		ar3.add("Test");//0
		ar3.add("DOM");//1
		ar3.add("Selenium");//2
		ar3.add('R');//3
		ar3.add(100);//4
		ar3.add(99.20);//5
		
		
		//to check the specific element of array list on the basis of index
		
		System.out.println(ar3.get(1));
		
		System.out.println("***********************");
		
        ArrayList ar4=new ArrayList();
		
		ar3.add("Test");//0
		ar3.add("DOM");//1
		ar3.add("Selenium");//2
		ar3.add('R');//3
		ar3.add(100);//4
		ar3.add(99.20);//5
		
	ArrayList ar5=new ArrayList();
	
	ArrayList ar3=new ArrayList();
		
		ar4.add("Data Configuration");
		ar4.add("Page Object Model");
		
		
	}

}
